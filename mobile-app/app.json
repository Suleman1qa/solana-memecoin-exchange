{
  "expo": {
    "name": "SolMemeEx",
    "scheme": "solmemeex",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/memecoinEx-logo.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/memecoinEx-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.solmemeex",
      "jsEngine": "hermes"
    },
    "android": {
      "package": "com.anonymous.solmemeex",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/memecoinEx-logo.png",
        "backgroundColor": "#ffffff"
      },
      "jsEngine": "hermes"
    },
    "plugins": ["expo-secure-store"],
    "extra": {
      "eas": {
        "projectId": "your-project-id"
      }
    }
  }
}
